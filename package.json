{
  "name": "@secrets/vue-mention",
  "version": "0.0.4",
  "description": "vue-mention",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/secrets-pro/vue-mention.git"
  },
  "homepage": "https://github.com/secrets-pro/vue-mention",
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.js",
    "build": "vue-cli-service build --target lib --name vue-mention ./src/index.js --inline-vue",
    "lint": "vue-cli-service lint",
    "release": "npm publish --access public --registry  https://registry.npmjs.org/"
  },
  "license": "MIT",
  "main": "dist/vue-mention.common.min.js",
  "module": "dist/vue-mention.common.min.js",
  "unpkg": "dist/vue-mention.common.min.js",
  "dependencies": {
    "vue": "^2.6.10"
  },
  "keywords":["vue","mention"],
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "vue-template-compiler": "^2.5.21"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "jsdelivr": "dist/vue-mention.common.min.js",
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
